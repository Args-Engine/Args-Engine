# GNU Make workspace makefile autogenerated by Premake

ifndef config
  config=debug32
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug32)
  args_core_config = debug32
endif
ifeq ($(config),release32)
  args_core_config = release32
endif
ifeq ($(config),debug64)
  args_core_config = debug64
endif
ifeq ($(config),release64)
  args_core_config = release64
endif

PROJECTS := args-core

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

args-core:
ifneq (,$(args_core_config))
	@echo "==== Building args-core ($(args_core_config)) ===="
	@${MAKE} --no-print-directory -C core -f Makefile config=$(args_core_config)
endif

clean:
	@${MAKE} --no-print-directory -C core -f Makefile clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  debug32"
	@echo "  release32"
	@echo "  debug64"
	@echo "  release64"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   args-core"
	@echo ""
	@echo "For more information, see https://github.com/premake/premake-core/wiki"