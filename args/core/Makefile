OBJECTS := \
	$(OBJDIR)/ecsregistry.o \
	$(OBJDIR)/entity.o \
	$(OBJDIR)/engine.o \
	$(OBJDIR)/module.o \
	$(OBJDIR)/artifact_cache.o \
	$(OBJDIR)/strpath_manip.o \
